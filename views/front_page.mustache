	<div class="menu-btn"><div class="menu-btn_burger"></div></div>
	<div class="menu-options_color"></div>
	<ul class="menu-options">
		<li><div class="container menu-button" id="about me"><button>about me</button></div></li>
		<li><div class="container menu-button" id="current works"><button>current work</button></div></li>
		<li><div class="container menu-button" id="old projects"><button>old projects</button></div></li>
	</ul>
	<hr class="headline">
	<h1 class="main-header">Hi</h1>
	<h1 class="main-header" style="margin-top: 40px; margin-left: 30px">I'm {{NAME}}</h1>
	<body>
		<div id="svgContainer" style="margin: 50px 50px;">{{#SVG_ROWS}}
			<svg id='svg{{SVG_NAME}}' width="0" height="0">{{#ROW_LINES}}
				<path
					id="path||{{CHILD_ELEMENT_NAME}}"
					d = "M0 0"
					style="
						stroke: #eb5810;
						fill: none;
						stroke-width: 5px;"/>
			{{/ROW_LINES}}</svg>
		{{/SVG_ROWS}}</div>
		<div class="project-popup">
			<div class="project-menu"><div class="project-menu_burger"></div></div>
			<h2 id="project-name"></h2>
			<hr>
			<div id="about-page">
				Where am I in life?
				<div class="profile-background">
					<div class="profile-picture">
						<img src="http://pm1.narvii.com/7564/d597f18ed75fb6c8875ee116b7f11652d8f05839r1-517-517v2_00.jpg">
						<div class="hex-rect fir"></div>
						<div class="hex-rect sec"></div>
						<div class="hex-rect thi"></div>

						<div class="hex-circ fir"></div>
						<div class="hex-circ sec"></div>
						<div class="hex-circ thi"></div>
						<div class="hex-circ fou"></div>
					</div>
					<ul class="profile-words">{{#PROFILE_WORDS}}
						<li>- {{PROFILE_WORD}}</li>
					{{/PROFILE_WORDS}}</ul>
				</div>
			</div>
			<div id="current-works-page">
				test current
			</div>
			<div id="old-page">{{{SPIDER_WEB}}}</div>
		</div>
